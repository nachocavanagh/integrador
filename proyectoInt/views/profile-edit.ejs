<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head.ejs') %> 

<body>
	<!-- include Header -->
	<%- include('./partials/header.ejs') %> 

	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<!-- Show the "ultimos" products -->
					
			<!-- Show the "ultimos" products -->
			<div class="main">
					
			<!-- Sign up form -->
				<section class="signup">
					<div class="register-container">
						<div class="signup-content">
							<div class="signup-form">
								<h2 class="form-title">Editar Perfil</h2>
								<form method="POST" class="register-form" id="register-form">
									<div class="form-group">
					
										<input type="text" name="name" id="name" placeholder="Nombre" />
									</div>
					
									<div class="form-group">
					
										<input type="email" name="email" id="email" placeholder="Apellido" />
									</div>
					
									<div class="form-group">
					
										<input type="password" name="pass" id="pass" placeholder="Email" />
									</div>
					
					
									<div class="form-group form-button">
										<input type="submit" name="signup" id="signup" class="form-submit"
															value="Confirmar" />
									</div>
								</form>
						</div>
						<div class="agregar-foto">
							<label class="form-label" for="customFile">
							</label>
								<input type="file" class="form-control" id="customFile" /> </label>
						</div>
					
					</div>
				</div>
			</section>
					
		</div>


	<!--include footer -->
	<%- include('./partials/footer.ejs') %> 

	<!-- /footer -->
</body>

</html>